
    - input_type: <%= @input_type %>
      paths:<% @paths.each do |path| %>
        - <%= path %>
      <%- end -%>
      encoding: <%= @encoding %>
<% if @exclude_lines -%>      exclude_lines: <%= @exclude_lines %> <% end -%>
<% if @include_lines -%>      include_lines: <%= @include_lines %> <% end -%>
<% if @exclude_files -%>      exclude_files: <%= @exclude_files %> <% end -%>

      fields_under_root: <%= @fields_under_root %>
      ignore_older: <%= @ignore_older %>
      document_type: <%= @document_type %>
      scan_frequency: <%= @scan_frequency %>
      harvester_buffer_size: <%= @harvester_buffer_size %>
      max_bytes: <%= @max_bytes %>
      tail_files: <%= @tail_files %>
      symlinks: <%= @symlinks %>
      backoff: <%= @backoff %>
      max_backoff: <%= @max_backoff %>
      backoff_factor: <%= @backoff_factor %>
      harvester_limit: <%= @harvester_limit %>
      fields:<% @fields.each do |key, value| %>
        <%= key -%>: <%= value %>
      <%- end -%>
      
